<field>
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "well"}) do |f| %>
  <%= devise_error_messages! %>
  <legend>Sign Up</legend>
  <div>
  	<%= f.input :first_name %></div>
  <div>
  	<%= f.input :last_name %> </div>
  <div>
  	<%= f.input :profile_name %> </div>

  <div>
  <%= f.input :email, autofocus: true %></div>

  <div>
    <%= f.input :password, autocomplete: "off" %></div>

  <div>
    <%= f.input :password_confirmation, autocomplete: "off" %></div>

  <div><%= f.button :submit, "Sign up" %></div>
<% end %>
</field>
<%= render "devise/shared/links" %>
